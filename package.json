{
  "name": "lazy-jack-flutter-logger-easy-life",
  "displayName": "lazyJ flutter colorfulLogLife",
  "description": "Brighten up your Flutter logs with ColorfulLogLife, making debugging more vibrant and effortless.",
  "version": "0.0.12",
  "publisher": "jackFan",
  "icon": "image/lazy_man.png",
  "homepage": "https://marketplace.visualstudio.com/items?itemName=jackFan.lazy-jack-flutter-logger-easy-life",
  "repository": {
    "type": "git",
    "url": "https://github.com/jack-fan1991/lazy-jack-flutter-logger-easy-life"
  },
  "bugs": {
    "url": "https://github.com/jack-fan1991/lazy-jack-flutter-logger-easy-life/issues"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [],
  "keywords": [
    "flutter",
    "logger"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "snippets": [],
    "configuration": {
      "title": "Flutter Logger Easy Life",
      "type": "object",
      "properties": {
        "FlutterLoggerEasyLife.customPrefix": {
          "type": "string",
          "default": "",
          "description": "Custom prefix for resolving file paths in debug logs.",
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "exclude": [
    "node_modules",
    ".vscode"
  ],
  "dependencies": {
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "axios": "^1.3.2",
    "change-case": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.19",
    "typescript": "^5.8.3",
    "vsce": "^1.97.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.194"
  }
}